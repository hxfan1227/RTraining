@article{ ISI:000317246200039,
Author = {Xu, Ligang and Xu, Jin and Jiang, Jiahu and Dong, Lei and You, Hailin},
Title = {{Impacts of Chelants on Transport of Pb in Soils and Vegetation System}},
Journal = {{DISASTER ADVANCES}},
Year = {{2013}},
Volume = {{6}},
Number = {{1}},
Pages = {{298-306}},
Month = {{MAR}},
Abstract = {{The soil heavy metal contamination, with a large range of source, which
   gives rise to terrific hazards to human activities, has been treated as
   a difficult and burning issue in the environmental pollution. Cheating
   induced phytoremediation technique, environmental and economic
   efficiently, prospects fairly in the realm of soil pollution
   remediation. The influence of chelant such as EDTA, citric acid and
   chitosan on the growth of the crop that contaminated by Pb had been
   studied in this paper. The dissolution of Pb affected by different
   chelants, the rule of the Pb transfer in the soil-crop system had been
   discussed. Meanwhile, the promotion of Pb accumulation in the crop
   triggered by coolant and the remediation efficiency of soil Pb
   contamination of unusual plants also has been analyzed. The conclusion
   indicates that the EDTA's cheating induction effect on the plants grown
   in soil contaminated by 400mg.kg(-1) Pb is prominent as the chelant
   concentration is 5mmol-kg(-1), and the remediation efficiency of maize
   is better than mustard, cowl and cabbage. The result may be a guide to
   decrease the hazard of Pb contamination in the soil-crop system.}},
