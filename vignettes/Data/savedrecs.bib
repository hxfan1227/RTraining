@article{ ISI:000390691700009,
Author = {Ye, Xuchun and Li, Xianghu and Xu, Chong-Yu and Zhang, Qi},
Title = {{Similarity, difference and correlation of meteorological and
   hydrological drought indices in a humid climate region - the Poyang Lake
   catchment in China}},
Journal = {{HYDROLOGY RESEARCH}},
